{
  "name": "ZED One Dark",
  "type": "dark",
  "colors": {
    "editor.background": "#25272d",
    "editor.foreground": "#acb2be",
    "editorBracketHighlight.foreground1": "#B3B9C5",
    "editorBracketHighlight.foreground2": "#ab7aca",
    "activityBarBadge.background": "#74ade8",
    "sideBarTitle.foreground": "#acb2be",
    "list.activeSelectionBackground": "#454a56",
    "list.inactiveSelectionBackground": "#363c46",
    "list.hoverBackground": "#363c46",
    "list.dropBackground": "#838994",
    "sideBar.background": "#2f343e",
    "sideBar.foreground": "#c8ccd4",
    "sideBar.border": "#464b57",
    "titleBar.activeBackground": "#3b414d",
    "titleBar.inactiveBackground": "#3b414d",
    "activityBar.activeBackground": "#3b414d",
    "activityBar.background": "#3b414d",
    "statusBar.background": "#3b414d",
    "statusBar.noFolderBackground": "#3b414d",
    "statusBar.foreground": "#c8ccd4",
    "tab.activeBorder": "#74ade8",
    "tab.activeBackground": "#202427",
    "tab.inactiveBackground": "#2f343e",
    "tab.activeForeground": "#c8ccd4",
    "tab.inactiveForeground": "#838994",
    "editorGroupHeader.tabsBackground": "#2f343e",
    "input.background": "#2e343e",
    "input.foreground": "#c8ccd4",
    "input.placeholderForeground": "#696B77",
    "inputOption.activeBorder": "#74ade8",
    "button.background": "#74ade8",
    "button.foreground": "#ffffff",
    "dropdown.background": "#2e343e",
    "dropdown.foreground": "#c8ccd4",
    "focusBorder": "#293b5b",
    "panel.background": "#2f343e",
    "panel.border": "#464b57",
    "badge.background": "#74ade8",
    "badge.foreground": "#ffffff",
    "quickInput.background": "#30343d",
    "quickInputList.focusBackground": "#464a55",
    "terminal.background": "#282c33",
    "terminal.foreground": "#c8ccd4",
    "terminal.ansiBlack": "#282c33",
    "terminal.ansiRed": "#d07277",
    "terminal.ansiGreen": "#a1c181",
    "terminal.ansiYellow": "#dec184",
    "terminal.ansiBlue": "#74ade8",
    "terminal.ansiMagenta": "#be5046",
    "terminal.ansiCyan": "#6eb4bf",
    "terminal.ansiWhite": "#c8ccd4"
  },
  "tokenColors": [
    {
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#979aa1"
      }
    },
    {
      "scope": [
        "string"
      ],
      "settings": {
        "foreground": "#a1c181"
      }
    },
    {
      "scope": [
        "constant.numeric",
        "constant.language",
        "constant.character",
        "constant.other"
      ],
      "settings": {
        "foreground": "#bf956a"
      }
    },
    {
      "scope": [
        "keyword",
        "storage",
        "storage.type"
      ],
      "settings": {
        "foreground": "#ab7aca"
      }
    },
    {
      "scope": [
        "support.type"
      ],
      "settings": {
        "foreground": "#6eb4bf"
      }
    },
    {
      "scope": [
        "entity.name.function",
        "support.function",
        "meta.function-call.generic"
      ],
      "settings": {
        "foreground": "#73ade9"
      }
    },
    {
      "scope": [
        "variable"
      ],
      "settings": {
        "foreground": "#c8ccd4"
      }
    },
    {
      "scope": [
        "variable.language",
        "variable.other",
        "variable.parameter.function-call.python"
      ],
      "settings": {
        "foreground": "#c8ccd4"
      }
    },
    {
      "scope": [
        "invalid"
      ],
      "settings": {
        "foreground": "#d07277"
      }
    },
    {
      "scope": [
        "entity.name.type",
        "entity.other.inherited-class"
      ],
      "settings": {
        "foreground": "#6eb4bf"
      }
    },
    {
      "scope": [
        "tag"
      ],
      "settings": {
        "foreground": "#74ade8"
      }
    },
    {
      "scope": "support.class.component",
      "settings": {
        "foreground": "#78A9B4"
      }
    },
    {
      "scope": [
        "entity.other.attribute-name",
        "entity.name.tag"
      ],
      "settings": {
        "foreground": "#80abe3"
      }
    },
    {
      "scope": [
        "variable.other.property",
        "variable.other.object.property",
        "meta.attribute"
      ],
      "settings": {
        "foreground": "#c37779"
      }
    },
    {
      "scope": [
        "keyword.operator",
        "storage.type.function.arrow"
      ],
      "settings": {
        "foreground": "#6EB4BF"
      }
    },
  ],
  "semanticHighlighting": true,
  "semanticTokenColors": {
    "variable": {
      "foreground": "#c8ccd4"
    },
    "parameter": {
      "foreground": "#c8ccd4"
    },
    "property": {
      "foreground": "#c37779"
    },
    "function": {
      "foreground": "#73ade9"
    },
    "class": {
      "foreground": "#6eb4bf"
    },
    "keyword": {
      "foreground": "#ab7aca"
    },
    "string": {
      "foreground": "#a1c181"
    },
    "number": {
      "foreground": "#bf956a"
    },
    "type": {
      "foreground": "#6eb4bf"
    }
  }
}
